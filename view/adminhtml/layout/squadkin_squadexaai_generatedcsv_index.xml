<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<update handle="styles"/>
	<head>
		<css src="Squadkin_SquadexaAI::css/product-create-modal.css"/>
		<css src="Squadkin_SquadexaAI::css/generatedcsv-grid.css"/>
		<script src="Squadkin_SquadexaAI::js/job-status-poll.js"/>
	</head>
	<body>
		<referenceContainer name="content">
			<block class="Squadkin\SquadexaAI\Block\Adminhtml\Messages\HtmlSuccessMessage" 
				   name="squadexa.html.success.message" 
				   template="Squadkin_SquadexaAI::messages/html-success-message.phtml" 
				   before="-"/>
			<container name="generatedcsv.grid.container" htmlTag="div" htmlClass="admin__data-grid-wrap admin__data-grid-wrap-static">
				<uiComponent name="squadkin_squadexaai_generatedcsv_listing"/>
			</container>
			<block class="Magento\Backend\Block\Template" name="job.status.poll.config" template="Squadkin_SquadexaAI::generatedcsv/job_poll_config.phtml" after="-"/>
		</referenceContainer>
	</body>
</page>
