<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">squadkin_aiautoproductbuilder_generatedcsv_listing.squadkin_aiautoproductbuilder_generatedcsv_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>squadkin_aiautoproductbuilder_generatedcsv_columns</spinner>
		<deps>
			<dep>squadkin_aiautoproductbuilder_generatedcsv_listing.squadkin_aiautoproductbuilder_generatedcsv_listing_data_source</dep>
		</deps>
	</settings>
	<dataSource name="squadkin_aiautoproductbuilder_generatedcsv_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">generatedcsv_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Squadkin_AIAutoProductBuilder::GeneratedCsv</aclResource>
		<dataProvider name="squadkin_aiautoproductbuilder_generatedcsv_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>generatedcsv_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="squadkin_aiautoproductbuilder_generatedcsv_columns">
		<selectionsColumn name="ids">
			<settings>
				<indexField>generatedcsv_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="generatedcsv_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="input_file_name">
			<settings>
				<filter>text</filter>
				<label translate="true">Input File Name</label>
			</settings>
		</column>
		<actionsColumn name="input_file_download" class="Squadkin\AIAutoProductBuilder\Ui\Component\Listing\Column\InputFileActions">
			<settings>
				<label translate="true">Input File Download</label>
				<sortable>false</sortable>
			</settings>
		</actionsColumn>
		<column name="response_file_name">
			<settings>
				<filter>text</filter>
				<label translate="true">Response File Name</label>
			</settings>
		</column>
		<actionsColumn name="response_file_download" class="Squadkin\AIAutoProductBuilder\Ui\Component\Listing\Column\OutputFileActions">
			<settings>
				<label translate="true">Output File Download</label>
				<sortable>false</sortable>
			</settings>
		</actionsColumn>
		<column name="import_status">
			<settings>
				<filter>select</filter>
				<options class="Squadkin\AIAutoProductBuilder\Model\Config\Source\ImportStatus"/>
				<dataType>select</dataType>
				<label translate="true">Import Status</label>
			</settings>
		</column>
		<column name="imported_products_count">
			<settings>
				<filter>textRange</filter>
				<label translate="true">Imported Products</label>
			</settings>
		</column>
		<column name="imported_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
			<settings>
				<filter>dateRange</filter>
				<dataType>date</dataType>
				<label translate="true">Import Date</label>
			</settings>
		</column>
		<actionsColumn name="import_actions" class="Squadkin\AIAutoProductBuilder\Ui\Component\Listing\Column\ImportActions">
			<settings>
				<label translate="true">Import Actions</label>
				<sortable>false</sortable>
			</settings>
		</actionsColumn>
		<column name="created_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
			<settings>
				<filter>dateRange</filter>
				<dataType>date</dataType>
				<label translate="true">Created At</label>
			</settings>
		</column>
	</columns>
</listing>
