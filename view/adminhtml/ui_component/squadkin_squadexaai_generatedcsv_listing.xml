<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">squadkin_squadexaai_generatedcsv_listing.squadkin_squadexaai_generatedcsv_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>squadkin_squadexaai_generatedcsv_columns</spinner>
		<deps>
			<dep>squadkin_squadexaai_generatedcsv_listing.squadkin_squadexaai_generatedcsv_listing_data_source</dep>
		</deps>
	</settings>
	<dataSource name="squadkin_squadexaai_generatedcsv_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">generatedcsv_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Squadkin_SquadexaAI::GeneratedCsv</aclResource>
		<dataProvider name="squadkin_squadexaai_generatedcsv_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>generatedcsv_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<massaction name="listing_massaction">
			<action name="delete">
				<settings>
					<confirm>
						<message translate="true">Are you sure you want to delete selected item(s) and their associated files?</message>
						<title translate="true">Delete items</title>
					</confirm>
					<url path="squadkin_squadexaai/generatedcsv/massDelete"/>
					<type>delete</type>
					<label translate="true">Delete</label>
				</settings>
			</action>
		</massaction>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="squadkin_squadexaai_generatedcsv_columns">
		<selectionsColumn name="ids">
			<settings>
				<indexField>generatedcsv_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="generatedcsv_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="input_file_name">
			<settings>
				<filter>text</filter>
				<label translate="true">Input File Name</label>
			</settings>
		</column>
		<actionsColumn name="input_file_download" class="Squadkin\SquadexaAI\Ui\Component\Listing\Column\InputFileActions">
			<settings>
				<label translate="true">Input File Download</label>
				<sortable>false</sortable>
			</settings>
		</actionsColumn>
		<column name="response_file_name">
			<settings>
				<filter>text</filter>
				<label translate="true">Response File Name</label>
			</settings>
		</column>
		<actionsColumn name="response_file_download" class="Squadkin\SquadexaAI\Ui\Component\Listing\Column\OutputFileActions">
			<settings>
				<label translate="true">Output File Download</label>
				<sortable>false</sortable>
			</settings>
		</actionsColumn>
		<column name="generation_type" component="Magento_Ui/js/grid/columns/select">
			<settings>
				<filter>select</filter>
				<dataType>select</dataType>
				<label translate="true">Generation Type</label>
				<options>
					<option name="single" xsi:type="array">
						<item name="value" xsi:type="string">single</item>
						<item name="label" xsi:type="string" translate="true">Single Product</item>
					</option>
					<option name="csv" xsi:type="array">
						<item name="value" xsi:type="string">csv</item>
						<item name="label" xsi:type="string" translate="true">CSV Upload</item>
					</option>
				</options>
			</settings>
		</column>
		<column name="created_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
			<settings>
				<filter>dateRange</filter>
				<dataType>date</dataType>
				<label translate="true">Created At</label>
			</settings>
		</column>
		<column name="job_id">
			<settings>
				<visible>false</visible>
				<filter>text</filter>
				<label translate="true">Job ID</label>
			</settings>
		</column>
		<column name="import_status" component="Magento_Ui/js/grid/columns/select">
			<settings>
				<filter>select</filter>
				<dataType>select</dataType>
				<label translate="true">Status</label>
				<options>
					<option name="pending" xsi:type="array">
						<item name="value" xsi:type="string">pending</item>
						<item name="label" xsi:type="string" translate="true">Pending</item>
					</option>
					<option name="processing" xsi:type="array">
						<item name="value" xsi:type="string">processing</item>
						<item name="label" xsi:type="string" translate="true">Processing</item>
					</option>
					<option name="completed" xsi:type="array">
						<item name="value" xsi:type="string">completed</item>
						<item name="label" xsi:type="string" translate="true">Completed</item>
					</option>
					<option name="failed" xsi:type="array">
						<item name="value" xsi:type="string">failed</item>
						<item name="label" xsi:type="string" translate="true">Failed</item>
					</option>
				</options>
			</settings>
		</column>
		<actionsColumn name="actions" class="Squadkin\SquadexaAI\Ui\Component\Listing\Column\GeneratedCsvActions">
			<settings>
				<indexField>generatedcsv_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
